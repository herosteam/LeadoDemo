<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_height="wrap_content"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/mountains"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/ic_maountains" />

    <ImageView
        android:id="@+id/line2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginBottom="16dp"
        app:layout_constraintBottom_toTopOf="@+id/mind"
        app:layout_constraintEnd_toStartOf="@+id/mountains"
        app:layout_constraintStart_toStartOf="@+id/mountains"
        app:srcCompat="@drawable/ic_line_2" />

    <ImageView
        android:id="@+id/line3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="8dp"
        android:layout_marginBottom="40dp"
        app:layout_constraintBottom_toBottomOf="@+id/mind"
        app:layout_constraintEnd_toStartOf="@+id/mind"
        app:layout_constraintStart_toStartOf="@+id/line2"
        app:srcCompat="@drawable/ic_line_3" />

    <ImageView
        android:id="@+id/mind"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="40dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/mountains"
        app:srcCompat="@drawable/ic_mind_icon" />

    <ImageView
        android:id="@+id/line4"
        android:layout_width="150dp"
        android:layout_height="wrap_content"
        android:layout_marginStart="95dp"
        android:layout_marginTop="75dp"
        android:rotation="90"
        android:scaleY="-1"
        app:layout_constraintBottom_toBottomOf="@+id/mind"
        app:layout_constraintStart_toEndOf="@+id/line3"
        app:layout_constraintTop_toTopOf="@+id/mind"
        app:srcCompat="@drawable/ic_line_4" />

    <ImageView
        android:id="@+id/line5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="95dp"
        android:layout_marginTop="70dp"
        android:scaleX="-1"
        app:layout_constraintEnd_toStartOf="@+id/line4"
        app:layout_constraintStart_toEndOf="@+id/mind"
        app:layout_constraintTop_toTopOf="@+id/line4"
        app:srcCompat="@drawable/ic_line_5" />

    <ImageView
        android:id="@+id/tree"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="16dp"
        app:layout_constraintBottom_toBottomOf="@+id/line5"
        app:layout_constraintStart_toEndOf="@+id/line5"
        app:layout_constraintTop_toTopOf="@+id/line5"
        app:srcCompat="@drawable/ic_tree" />

    <ImageView
        android:id="@+id/self_awareness"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="90dp"
        app:layout_constraintBottom_toTopOf="@+id/line6"
        app:layout_constraintEnd_toStartOf="@+id/line5"
        app:layout_constraintTop_toBottomOf="@+id/mind"
        app:srcCompat="@drawable/ic_self_awarness_icon" />

    <ImageView
        android:id="@+id/clouds"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintBottom_toBottomOf="@+id/self_awareness"
        app:layout_constraintEnd_toStartOf="@+id/self_awareness"
        app:layout_constraintTop_toTopOf="@+id/self_awareness"
        app:srcCompat="@drawable/ic_clouds" />

    <ImageView
        android:id="@+id/lines"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="20dp"
        android:layout_marginBottom="32dp"
        app:layout_constraintBottom_toBottomOf="@+id/mind"
        app:layout_constraintStart_toEndOf="@+id/mind"
        app:layout_constraintTop_toTopOf="@+id/mind"
        app:srcCompat="@drawable/ic_lines" />

    <Button
        android:id="@+id/button1"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="180dp"
        android:background="@drawable/button_shape"
        android:backgroundTint="@android:color/white"
        android:clickable="true"
        android:focusable="true"
        android:text="Personal-Branding"
        android:padding="10dp"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="17sp"
        app:layout_constraintBottom_toBottomOf="@+id/mountains"
        app:layout_constraintEnd_toEndOf="@+id/mountains"
        app:layout_constraintStart_toStartOf="@+id/mountains"
        app:layout_constraintTop_toBottomOf="@+id/mountains" />

    <Button
        android:id="@+id/button2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="60dp"
        android:background="@drawable/button_shape"
        android:backgroundTint="@android:color/white"
        android:clickable="true"
        android:focusable="true"
        android:text="Mind-Mapping"
        android:padding="10dp"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/mind"
        app:layout_constraintStart_toStartOf="@+id/mind"
        app:layout_constraintTop_toTopOf="@+id/mind" />

    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="180dp"
        android:layout_marginTop="80dp"
        android:background="@drawable/button_shape"
        android:backgroundTint="@android:color/white"
        android:clickable="true"
        android:focusable="true"
        android:text="Self-Awareness"
        android:padding="10dp"
        android:textAllCaps="false"
        android:textColor="#000000"
        android:textSize="18sp"
        app:layout_constraintBottom_toBottomOf="@+id/self_awareness"
        app:layout_constraintEnd_toEndOf="@+id/self_awareness"
        app:layout_constraintStart_toStartOf="@+id/self_awareness"
        app:layout_constraintTop_toTopOf="@+id/self_awareness" />

    <ImageView
        android:id="@+id/line6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginEnd="50dp"
        android:rotation="90"
        android:scaleX="-1"
        android:scaleY="-1"
        app:layout_constraintEnd_toStartOf="@+id/button3"
        app:layout_constraintTop_toBottomOf="@+id/line5"
        app:srcCompat="@drawable/ic_line_6" />

    <ImageView
        android:id="@+id/line7"
        android:layout_width="90dp"
        android:layout_height="80dp"
        android:layout_marginEnd="27dp"
        android:layout_marginBottom="21dp"
        android:rotation="90"
        app:layout_constraintBottom_toBottomOf="@+id/line6"
        app:layout_constraintEnd_toStartOf="@+id/line6"
        app:srcCompat="@drawable/line_7" />

</androidx.constraintlayout.widget.ConstraintLayout>